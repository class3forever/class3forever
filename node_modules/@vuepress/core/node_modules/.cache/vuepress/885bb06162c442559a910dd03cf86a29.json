sh({\r\n              value: i,\r\n              enable: true,\r\n            })\r\n          }\r\n          paginators.push({\r\n            value: '...',\r\n            enable: false,\r\n          })\r\n          paginators.push({\r\n            value: this.lastPage,\r\n            enable: true,\r\n          })\r\n        }\r\n      }\r\n      return paginators\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    nextPage () {\r\n      this.setPage(this.currentPage + 1)\r\n    },\r\n\r\n    prevPage () {\r\n      this.setPage(this.currentPage - 1)\r\n    },\r\n\r\n    setPage (targetPage) {\r\n      if (targetPage <= this.lastPage && targetPage >= this.firstPage) {\r\n        this.$emit('input', targetPage)\r\n      }\r\n    },\r\n  },\r\n}\r\n"]}]}